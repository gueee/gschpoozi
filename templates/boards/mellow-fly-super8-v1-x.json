      "enable_pin": "PG1",
      "uart_pin": "PF15",
      "cs_pin": "PF15"
    },
    "MOTOR_3": {
      "label": "Driver 3 (DRIVE3)",
      "step_pin": "PE14",
      "dir_pin": "PE8",
      "enable_pin": "PE9",
      "uart_pin": "PE7",
      "cs_pin": "PE7"
    },
    "MOTOR_4": {
      "label": "Driver 4 (DRIVE4)",
      "step_pin": "PE15",
      "dir_pin": "PE11",
      "enable_pin": "PF2",
      "uart_pin": "PE10",
      "cs_pin": "PE10"
    },
    "MOTOR_5": {
      "label": "Driver 5 (DRIVE5)",
      "step_pin": "PE1",
