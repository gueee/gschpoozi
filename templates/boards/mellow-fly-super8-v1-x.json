    "MOTOR_6": {
      "label": "Driver 6 (DRIVE6)",
      "step_pin": "PE0",
      "dir_pin": "PG3",
      "enable_pin": "PG4",
      "uart_pin": "PG2",
      "cs_pin": "PG2"
    },
    "MOTOR_7": {
      "label": "Driver 7 (DRIVE7)",
      "step_pin": "PE6",
      "dir_pin": "PG6",
      "enable_pin": "PG7",
      "uart_pin": "PG5",
      "cs_pin": "PG5"
    }
