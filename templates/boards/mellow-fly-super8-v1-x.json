  "heater_ports": {
    "HEAT0": {
      "label": "Heater 0 (Hotend)",
      "pin": "PB0",
      "max_current_amps": 3
    },
    "HEAT1": {
      "label": "Heater 1",
      "pin": "PB1",
      "max_current_amps": 3
    },
    "HEAT2": {
      "label": "Heater 2",
      "pin": "PC7",
      "max_current_amps": 3
    },
    "HEAT3": {
      "label": "Heater 3",
      "pin": "PF7",
      "max_current_amps": 3
    },
    "HEAT4": {
      "label": "Heater 4",
      "pin": "PF6",
      "max_current_amps": 3
    },
    "BED": {
      "label": "Heated Bed",
      "pin": "PE5",
      "max_current_amps": 15
    }
  },

  "thermistor_ports": {
    "ADC_0": {
      "label": "Thermistor 0 (T0/Hotend)",
      "pin": "PF4"
    },
    "ADC_1": {
      "label": "Thermistor 1 (T1/Chamber)",
      "pin": "PF5"
    },
    "ADC_2": {
      "label": "Thermistor 2 (T2)",
      "pin": "PF9"
