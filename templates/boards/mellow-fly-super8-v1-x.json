      "pin": "PG11"
    },
    "IO2": {
      "label": "IO2 (Endstop)",
      "pin": "PG10"
    },
    "IO3": {
      "label": "IO3 (Endstop)",
      "pin": "PG9"
    },
    "IO4": {
      "label": "IO4 (Endstop)",
      "pin": "PD7"
    },
    "IO5": {
      "label": "IO5 (Endstop)",
      "pin": "PD6"
    },
    "IO6": {
      "label": "IO6 (Endstop)",
      "pin": "PA8"
