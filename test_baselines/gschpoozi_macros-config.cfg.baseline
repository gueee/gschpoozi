[gcode_macro _MACRO_CONFIG]
description: Configuration variables for START_PRINT and END_PRINT macros
# Printer Dimensions (from wizard)
variable_bed_size_x: 350
variable_bed_size_y: 350
variable_bed_size_z: 300
variable_bed_center_x: 175
variable_bed_center_y: 175
# Probe and Leveling
variable_probe_type: "none"
variable_eddy_mesh_method: "none"
variable_leveling_method: "none"
# Klipper parses gcode_macro variables with Python literal_eval, so booleans must be True/False (not true/false).
variable_leveling_enabled: False
variable_z_calibration_enabled: True
variable_always_home_z: False
# Heat Soak
variable_heat_soak_time: 0
# Chamber Heating
variable_chamber_temp_default: 0
# Bed Mesh
variable_bed_mesh_mode: "adaptive"
# Nozzle Cleaning (Brush)
variable_brush_enabled: False
variable_brush_x: 50.0
variable_brush_y: 317.0
variable_brush_z: 1.0
variable_brush_width: 30.0
variable_wipe_count: 3
# Purge Settings
variable_purge_style: "adaptive"
variable_purge_amount: 30.0
variable_purge_x: 5
variable_purge_y: 5
# Bucket (for blob/voron purge)
variable_bucket_x: 175
variable_bucket_y: 345
variable_bucket_z: 5.0
# LED Status
variable_led_enabled: False
variable_led_name: "status_led"
# Park Position (END_PRINT)
variable_park_position: "front"
variable_park_z_hop: 10.0
# Default park_z_max should scale with printer Z travel.
# Use (bed_size_z - 5) as a sane cap (also consistent with z_max - 5 safety margin in the macro).
variable_park_z_max: 295.0
